!!!
%html
  %head
    %title MateApp
    = typekit 'gfk5ghe'
    %script{:src => "https://maps.googleapis.com/maps/api/js?key=#{ENV['APP_GOOGLEMAPS']}", :type => "text/javascript"}
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    %header
      = render partial: "/layouts/navbar"
      %p.notice= notice
      %p.alert= alert
    %main
      = yield
    %footer
